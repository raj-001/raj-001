print("Addition")
x=int(input("Enter Number 1: "))
y=int(input("Enter Number 2: "))
print("The Sum is = ",x+y)
print("Subtraction")
p=int(input("Enter Number 1: "))
q=int(input("Enter Number 2: "))
print("The difference is = ",p-q)
print("Multiplication")
r=int(input("Enter Number 1: "))
s=int(input("Enter Number 2: "))
print("Product is = ",r*s)
print("Division")
m=int(input("Enter Number 1: "))
n=int(input("Enter Number 2: "))
print("Remainder is = ",m/n)
print("Thank You")
